				<key>ActionName</key>
				<string>Run AppleScript</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>on run {input, parameters}	if (count of input) â‰¤ 1 then		tell application "Finder"						set fileInfoList to {name, name extension} of every file of alias (input as string)			set inputPath to input as string						set listSize to length of (item 1 of fileInfoList)			set i to 1						repeat listSize times				if "exe" is equal to item i of (item 2 of fileInfoList) then					try						make new folder at input with properties {name:"Windows Applications"}					end try					move inputPath &amp; item i of (item 1 of fileInfoList) to inputPath &amp; "Windows Applications"									else if "png" is equal to item i of (item 2 of fileInfoList) or "jpg" is equal to item i of (item 2 of fileInfoList) then					try						make new folder at input with properties {name:"Pictures"}					end try					move inputPath &amp; item i of (item 1 of fileInfoList) to inputPath &amp; "Pictures"									else if "PDF" is equal to item i of (item 2 of fileInfoList) then					try						make new folder at input with properties {name:"Documents"}					end try					move inputPath &amp; item i of (item 1 of fileInfoList) to inputPath &amp; "Documents"									else if "mkv" is equal to item i of (item 2 of fileInfoList) or "mp4" is equal to item i of (item 2 of fileInfoList) then					try						make new folder at input with properties {name:"Movies"}					end try					move inputPath &amp; item i of (item 1 of fileInfoList) to inputPath &amp; "Movies"									else if "app" is equal to item i of (item 2 of fileInfoList) then					try						make new folder at input with properties {name:"MacOS Applications"}					end try					move inputPath &amp; item i of (item 1 of fileInfoList) to inputPath &amp; "MacOS Applications"									else if "" is equal to item i of (item 2 of fileInfoList) then					try						make new folder at input with properties {name:"Unknown"}					end try					move inputPath &amp; item i of (item 1 of fileInfoList) to inputPath &amp; "Unknown"									end if				set i to (i + 1)			end repeat		end tell	end ifend run</string>
